// Write the runSimulation method

public int runSimulation(){
    int count = 0;
    int roll1 = 0;
    int roll2 = 0;

    for(int  = 0; i < numSampleSize; i++){
        roll1  = roll();
        roll2 = roll();
        if(roll1 == roll2){
            count++;
        }
    }

    return (count/numSamplesize) * 10;
}
