//Write the SeatingChart Constructor

SeatingChart(Names[] names, int rows, int cols){
    int j =0; //rows
    int k =0; // cols

    for (int i = 0; i < names.length();i++){
        String temp = names[i].getLastName + ", " + names[i].getFirstName();

        chart[j][k] = temp;
        k++;
        if (k == cols){
            k = 0;
            j++;
        }

    }
    
}

